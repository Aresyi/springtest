<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ydj.springtest.dao.AreaDao">
    <select id="queryArea" resultType="com.ydj.springtest.entity.Area">
        SELECT
        *
        FROM
        tss_job
        ORDER BY
        id DESC
    </select>

    <select id="countLog" resultType="java.lang.Integer">
        SELECT
        count(1)
        FROM
        tss_log
        where
        state = #{state}
    </select>

</mapper>